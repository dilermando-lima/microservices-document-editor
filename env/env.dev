
# =========== CUSTOM VARS
SECRET_JWT=secret-jwt

# =========== LOGGING
LOGGING_LEVEL_ACCOUNT=DEBUG
LOGGING_LEVEL_SESSION=DEBUG
LOGGING_LEVEL_APICORE_INTEGRATION=DEBUG
LOGGING_PATTERN_CONSOLE="%d{HH:mm:ss.SSS} %5p %40logger{36} : %msg%n"

# =========== SERVER_PORT
ACCOUNT_SERVER_PORT=8089
SESSION_SERVER_PORT=8088

# =========== ENDPOINT
ACCOUNT_MS_ENDPOINT=http://localhost:${ACCOUNT_SERVER_PORT}
SESSION_MS_ENDPOINT=http://localhost:${SESSION_SERVER_PORT}

# =========== SWAGGER
SPRINGDOC_SWAGGER_UI_URLS_0_NAME=account
SPRINGDOC_SWAGGER_UI_URLS_0_URL=${ACCOUNT_MS_ENDPOINT}/swagger/api-docs

SPRINGDOC_SWAGGER_UI_URLS_1_NAME=session
SPRINGDOC_SWAGGER_UI_URLS_1_URL=${SESSION_MS_ENDPOINT}/swagger/api-docs

